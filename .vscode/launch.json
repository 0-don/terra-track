{
  "version": "1.0.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rustscan'",
      "cargo": {
        "args": ["build", "--bin=rustscan", "--package=rustscan"],
        "filter": {
          "name": "rustscan",
          "kind": "bin"
        }
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}
